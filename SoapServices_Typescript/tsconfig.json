{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": false,
		"sourceMap": true,
		"baseUrl": "./typescripts",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"outDir": "./dist/out-tsc",
		"types": [
			"jasmine",
			"jasminewd2",
			"node"
		],
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2017"
		],
		"paths": {
			"@e2e/*": [
				"./typescripts/*"
			]
		}
	}
}